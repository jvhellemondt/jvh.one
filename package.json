{
  "name": "jvh.one.react",
  "version": "1.0.0",
  "private": true,
  "description": "jvh.one.react",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "tsc --noEmit && eslint --fix --no-error-on-unmatched-pattern src/**/*.{js,jsx,ts,tsx}",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts{,x}": [
      "tsc --noEmit",
      "eslint --fix --quiet --no-error-on-unmatched-pattern src/**/*.{js,jsx,ts,tsx}",
      "git add"
    ]
  },
  "dependencies": {
    "@types/lodash.kebabcase": "^4.1.6",
    "classnames": "^2.3.1",
    "gatsby": "^3.4.1",
    "gatsby-image": "^3.7.1",
    "gatsby-plugin-google-analytics": "^3.6.0",
    "gatsby-plugin-manifest": "^3.6.0",
    "gatsby-plugin-react-helmet": "^4.6.0",
    "gatsby-plugin-sass": "^4.6.0",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-source-filesystem": "^3.6.0",
    "gatsby-transformer-remark": "^4.4.1",
    "gatsby-transformer-sharp": "^3.7.1",
    "lodash.kebabcase": "^4.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.34.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/typescript": "1.22.1",
    "@types/react-helmet": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "7.29.0",
    "eslint-config-airbnb": "18.2.1",
    "gatsby-plugin-react-svg": "^3.0.1",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "typescript": "^4.3.2"
  }
}
